    {
      "id": "3BWn7c2UXrJo4IlcAbbSJ",
      "type": "text",
      "x": 1397,
      "y": 116,
      "width": 331,
      "height": 680,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
